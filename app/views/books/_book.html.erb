<div class="GridLex-col-3_sm-4_xs-12 mb-30">
  <div class="package-grid-item">
      <div class="image">
        <img src="<%= book.content %>" alt="Tour Package" />
        <div class="absolute-in-image">
          <div class="duration"><span>デザイン</span></div>
        </div>
      </div>
      <div class="content clearfix">
        <h5><%= book.title %></h5>
        <div class="rating-wrapper">
          <div class="raty-wrapper">
            <div class="star-rating-read-only" data-rating-score="4.0"></div> <span> / 7 review</span>
          </div>
        </div>
        <div class="absolute-in-content">
          <span class="btn"><i class="fa fa-heart-o"></i></span>
          <div class="price"><%= book.price %></div>
        </div>
      </div>
  </div>
</div>

